	<queryString>
		<![CDATA[SELECT top (1) ttd.Email, ttd.Nombre, ttc.Documento as cedula, ttc.Referencia_Pago as Cuenta_Numero,
 ttca.Saldo, ttca.Interes_Mora, ttca.honorarios, ttc.Valor_Pactado as Obligacion_Total, ttc.Fecha_Gestion, ttc.Fecha_Actualizacion, ttc.Fecha_Compromiso, ttca.Fecha_Actualizo
FROM telcos.ean.colombia.compromiso ttc with(nolock)
inner join telcos.ean.colombia.direccion ttd with(nolock) on ttd.Referencia_Pago = ttc.Referencia_Pago
inner join telcos.ean.colombia.cartera ttca with(nolock) on ttca.Referencia_Pago = ttc.Referencia_Pago
where ttd.Referencia_Pago = $P{Referencia}  order by Fecha_Actualizo desc]]>
	</queryString>
